dim oUser
set method.User = nothing

for each oUser in this.GetObjectsByTypeAndPropertyValue(Schema.Modules.Leviton.Classes.User.Path, "UserNumber", method.UserNumber)
	set method.User = oUser
	' found user exit loop
	exit for
next
set oUser = nothing]]>
