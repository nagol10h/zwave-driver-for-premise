						<Object ID="{5C5A6C9B-BC71-4538-84EA-0529A9ABE810}" Name="OnChangeWakeupInterval" Class="sys://Schema/Scripting/ClassProperty" Property="{04C263C1-BFF1-4241-83A7-E2BC8AB1F7D4}" >
